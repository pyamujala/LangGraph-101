# LangChain related environment variables

# API key for LangChain
LANGCHAIN_API_KEY=your_api_key_here

# Base URL for LangChain API
LANGCHAIN_API_BASE_URL=https://api.langchain.com

# Timeout for API requests (in seconds)
LANGCHAIN_API_TIMEOUT=30

# Log level for LangChain (DEBUG, INFO, WARNING, ERROR, CRITICAL)
LANGCHAIN_LOG_LEVEL=INFO

# GROQ related environment variables

# API key for GROQ
GROQ_API_KEY=your_groq_api_key_here

# Base URL for GROQ API
GROQ_API_BASE_URL=https://api.groq.com

# Timeout for GROQ API requests (in seconds)
GROQ_API_TIMEOUT=30

# Log level for GROQ (DEBUG, INFO, WARNING, ERROR, CRITICAL)
GROQ_LOG_LEVEL=INFO